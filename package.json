{
  "name": "proglearning-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "lint": "next lint",
    "format": "eslint --fix './{pages,src}/**/*.{js,jsx,ts,tsx}' && prettier --write --ignore-path .gitignore './{pages,src}/**/*.{js,jsx,ts,tsx}'",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "supabase-start": "dotenv -e .env -- supabase start",
    "supabase-stop": "dotenv -e .env -- supabase stop supabase stop --backup",
    "supabase-remove": "dotenv -e .env -- supabase stop supabase stop",
    "prod-db-push": "dotenv -e .env.production -- yarn prisma db push",
    "prod-migrate-deploy": "dotenv -e .env.production -- yarn prisma migrate deploy"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
